{
  "editor.fontSize": 16,
  "editor.rulers": [50, 60, 80],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ff4081"
  },
  "git.ignoreLimitWarning": true,
  "editor.tabSize": 2,
  "vsicons.associations.files": [
    {
      "icon": "test",
      "filename": false,
      "extensions": ["test.js"],
      "format": "svg",
      "overrides": "testjs"
    },
    {
      "icon": "coffeescript",
      "filename": false,
      "extensions": ["mock.js"],
      "format": "svg",
      "overrides": "coffeescript"
    },
    {
      "icon": "webpack",
      "filename": true,
      "extensions": [
        "webpack.pro.config.js",
        "webpack.debug.config.js",
        "webpack.base.config.js"
      ],
      "format": "svg",
      "overrides": "webpack"
    }
  ],
  "vsicons.associations.folders": [
    {
      "icon": "test",
      "extensions": ["@data-simulated"]
    }
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  // Set the default
  "editor.formatOnSave": false
}
