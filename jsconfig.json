{
  "typeAcquisition": {
    "include": ["jest"]
  },
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonJS",
    "paths": {
      "@dummydata*": ["__dummydata__/*"],
      "@mocks*": ["__mocks__/*"],

      "@Entry*": ["src/Entry/"],
      "@Helpers*": ["src/Helpers/"],

      "@Cross*": ["src/Cross/"],
      // "@GlobalPages*": ["src/Pages/Global/"],
      "styled-components*": ["node_modules/styled-components/"],
      "react*": ["node_modules/react/"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
